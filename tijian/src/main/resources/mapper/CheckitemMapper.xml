<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.demo.tijian.mapper.CheckitemMapper">

    <resultMap id="BaseResultMap" type="com.neuedu.demo.tijian.pojo.Checkitem">
            <id property="ciid" column="ciId" jdbcType="INTEGER"/>
            <result property="ciname" column="ciName" jdbcType="VARCHAR"/>
            <result property="cicontent" column="ciContent" jdbcType="VARCHAR"/>
            <result property="meaning" column="meaning" jdbcType="VARCHAR"/>
